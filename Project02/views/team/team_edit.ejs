<% include ../partials/header.ejs %>

<h1>Edit Team</h1>
<form method="POST" action="/team/update_team">
    <input type="hidden" name="team_id" value="<%= rs[0].team_id %>" />
    Team Name <input type="text" id="name" name="name" value="<%= rs[0].name%>"/>
    <br />
    Arena Name <input type="text" id="arena_name" name="arena_name" value="<%= rs[0].arena_name%>"/>
    <br />
    City <input type="text" id="city" name="city" value="<%= rs[0].city%>"/>
    <br />
    State <input type="text" id="state" name="state" value="<%= rs[0].state%>"/>
    <br />
    Zip Code <input type="text" id="zip" name="zip" value="<%= rs[0].zip%>"/>
    <br />
    <br/>
    <button id="editBtn">Edit</button>
</form>

<% include ../partials/footer.ejs %>